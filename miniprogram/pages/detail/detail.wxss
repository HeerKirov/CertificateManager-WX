.panel {
  margin-top: 12rpx;
  width: 90%;
}
.field {
  position: relative;
  min-height: 14px;
}
.space {
  height: 30rpx;
  width: 100%;
}
.field .label {
  width: 30%;
  font-size: 14px;
  color: #555555;
  position: absolute;
}
.field .content {
  width: 70%;
  position: relative;
  left: 30%;
  font-size: 14px;
}
.image-fields {
  width: 100%;
  height: 320rpx;
}
.image-field {
  float: left;
  width: 33%;
}
.image-field .title {
  width: 100%;
  color: #555555;
  font-size: 11px;
}
.image-field .image {
  width: 100%;
  padding: 4rpx;
  max-height: 320rpx;
}


.field .status.waiting {color: #999}
.field .status.passed {color: #090}
.field .status.not-pass {color: #f00}